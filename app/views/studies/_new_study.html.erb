<% remote_form_for (@study, :url => user_studies_path) do |f| %>
  <fieldset>
    <legend><%= %></legend>
    <%= error_messages_for 'study' %>
    <%= select("study", "university_id", University.find(:all).collect{ |c| [ c.title, c.id] }) %>
    <br />
    <%= date_select("study", "from", options = {:start_year => Time.now.year - 15} )%>
    <br />
    <%= date_select("study", "to", options = {:start_year => Time.now.year - 15} ) %>
    <br />
    <%= f.submit 'Create' %>
 </fieldset>
<% end %>
